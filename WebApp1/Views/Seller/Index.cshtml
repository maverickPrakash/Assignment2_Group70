@model IEnumerable<Product>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "_sellerPanelTemplate";
}
<h2 class="seller-head">List of Items</h2>
<hr />
<div class="seller-item">
@foreach(Product prop in Model){
<table class="item-table">
    <tr>
        <td>Item Name</td>
        <td>@prop.Name</td>
    </tr>
    <tr>
        <td>Description</td>
        <td>@prop.Description</td>
    </tr>
    <tr>
        <td>Asset Condition</td>
        <td>@prop.Asset_condition</td>
    </tr>
    <tr>
        <td>Category</td>
        <td>@prop.Category.CategoryName</td>
    </tr>
    <tr>
        <td>Start Date</td>
        <td>@prop.StartBidDate</td>
    </tr>
    <tr>
        <td>End Date</td>
        <td>@prop.ExpiryBidDate</td>
    </tr>
    <tr>
        <td>Cost</td>
        <td>@prop.Cost</td>
    </tr>
    <tr>
        <td>Image</td>
        <td>@prop.Image</td>
    </tr>
    <tr>
        <td><a asp-controller="Seller" asp-action="Edit" type="button" class="btn btn-secondary">Edit</a>    <a asp-controller="Seller" asp-action="Delete" asp-route-id="@prop.Id" asp-route-slug="@prop.Slug" class="btn btn-danger">Delete</a></td>
    </tr>
  </table>
  <br/><br/>
}
</div>